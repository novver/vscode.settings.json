{
    "files.autoSave": "off",
    "workbench.colorTheme": "Default Dark Modern",
    "workbench.startupEditor": "none",
    "workbench.editor.enablePreview": false,
    "workbench.statusBar.visible": false,
    "workbench.layoutControl.enabled": false,
    "workbench.tree.enableStickyScroll": false,
    "security.workspace.trust.enabled": false,
    "breadcrumbs.enabled": false,
    "editor.fontFamily": "JetBrains Mono, Geist Mono, monospace",
    "editor.fontWeight": "normal",
    "editor.letterSpacing": 0,
    "editor.tabSize": 4,
    "editor.fontSize": 18,
    "editor.lineHeight": 31,
    "editor.guides.bracketPairs": "active",
    "editor.guides.highlightActiveBracketPair": false,
    "editor.occurrencesHighlight": "off",
    "editor.autoClosingOvertype": "always",
    "editor.matchBrackets": "never",
    "editor.renderLineHighlight": "none",
    "editor.wordWrap": "on",
    "editor.guides.indentation": false,
    "editor.fontLigatures": true,
    "editor.minimap.enabled": false,
    "editor.stickyScroll.enabled": false,
    "editor.stickyScroll.scrollWithEditor": false,
    "editor.lightbulb.enabled": "off",
    "editor.inlineSuggest.enabled": false,
    //"editor.hover.enabled": false,
    "editor.hover.delay": 1500,
    "html.hover.references": false,
    "html.hover.documentation": false,
    "css.hover.references": false,
    "css.hover.documentation": false,
    "scss.hover.references": false,
    "scss.hover.documentation": false,
    "less.hover.references": false,
    "less.hover.documentation": false,
    "explorer.confirmDelete": false,
    "html.format.wrapAttributes": "auto",
    "html.format.wrapLineLength": 0,
    "html.format.contentUnformatted": "svg",
    "php.validate.enable": false,
    "window.commandCenter": false,
    "window.autoDetectColorScheme": false,
    "window.titleBarStyle": "custom",
    "window.menuBarVisibility": "compact",
    //
    "http.proxySupport": "off",
    "inlineChat.mode": "preview",
    "terminal.integrated.localEchoLatencyThreshold": -1,
    "terminal.integrated.localEchoEnabled": "off",
    "terminal.integrated.stickyScroll.enabled": false,
    "debug.console.fontSize": 12,
    "workbench.editorAssociations": {
        "*.md": "vscode.markdown.preview.editor"
    },
}
